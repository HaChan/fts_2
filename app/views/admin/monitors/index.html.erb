<div class="row">
  <div class="span6 offset3">
    <h2>Import</h2>
    <%= form_tag admin_export_request_path("table.csv"), method: "get" do %>
      <%= label_tag "Table" %>
      <%= select_tag "table", options_for_select(@tables) %>

      <%= submit_tag "Export", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
  <div class="span6 offset3">
    <h2>Import</h2>
    <%= form_tag admin_import_request_path, multipart: true do %>
      <%= select_tag "table", options_for_select(@tables) %>
      <%= file_field_tag :file %>
      <%= submit_tag "Import", class: "btn btn-small btn-primary" %>
    <% end %>
  </div>
</div>
