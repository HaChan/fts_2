<tr>
		<td class="listbox-entry">
			<%= question.subject.subject_name %>
		</td>
		<td class="listbox-entry" align="center">
		  <%= link_to question.subject_content, admin_question_path(question) %>
		</td>
		<td class="listbox-entry" align="center">
		  <%= question.type_question.question_type %>
		</td>
		<td class="listbox-header" align="center">
		  <%= link_to "Edit question", edit_admin_question_path(question.id),
		   class: "btn btn-small btn-primary" %>
		  <%= link_to "Delete question", admin_question_path(question.id),
		   method: :delete, class: "btn btn-small btn-primary" %>
		</td>
</tr>
